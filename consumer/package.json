    "@aws-sdk/lib-dynamodb": "^3.850.0",
    "@aws-sdk/util-dynamodb": "^3.592.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.138",
    "@types/node": "^20.14.2",
    "aws-lambda": "^1.0.7",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5"
  }
}
